{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [true, "never-prefix"],
    "semicolon": [true, "never"],
    "ter-indent": [true, 2],
    "no-console": false,
    "quotemark": [true, "single"]
  },
  "rulesDirectory": []
}
